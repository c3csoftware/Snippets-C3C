<aura:component implements="force:appHostable" access="global" controller="ContratoLoxamClubController">
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>

    <aura:attribute name="accountId" type="String" />
    <aura:attribute name="listTermoContrato" type="List" />
    <aura:attribute name="listAccount" type="List" />

    <aura:attribute name="component" type="Boolean" default="true"/>
    <aura:attribute name="termoContrato" type="Boolean" default="false"/>
    <aura:attribute name="contratoAceito" type="Boolean"/>
    <aura:attribute name="exceptionConstrucaoCivil" type="Boolean" default="false"/>
    <aura:attribute name="exceptionParticipanteLoxamClub" type="Boolean" default="false"/>
    <aura:attribute name="exceptionAccountNaoEncontrada" type="Boolean" default="false"/>
    <aura:attribute name="loading" type="Boolean" default="false"/>
    
	<ltng:require scripts="https://mozilla.github.io/pdf.js/build/pdf.js" />
    
    <aura:if isTrue="{!v.component}">
      <div class="contrato">
          <aura:if isTrue="{!v.loading}">
              <lightning:spinner />
          </aura:if>

          <aura:if isTrue="{!v.exceptionConstrucaoCivil}">
            <c:ExceptionContratoLoxamClub naoConstrucaoCivil="true"/>
          </aura:if>
          <aura:if isTrue="{!v.exceptionParticipanteLoxamClub}">
            <c:ExceptionContratoLoxamClub loxamClub="true"/>
          </aura:if>
          <aura:if isTrue="{!v.exceptionAccountNaoEncontrada}">
            <c:ExceptionContratoLoxamClub contaNaoEncontrada="true"/>
          </aura:if>

          <aura:if isTrue="{!v.termoContrato}">
            <lightning:card class="contrato-escopo">
                
                <div class="contrato-titulo">Regras Loxam Club</div>
                <lightning:card class="contrato-body">
                    <!-- <c:ContratoLoxamClubPDF pdf="{!$Resource.ContratoLoxamClubPDF}" versionId="1"/> -->
                    <div class="contrato-pdf">
                    	<iframe src="{!$Resource.ContratoLoxamClubPDF}" allowfullscreen="true" width="800" height="780" style="border: none;"></iframe> 
                    </div>
                    <!-- <aura:iteration items="{!v.listTermoContrato}" var="termoContrato">
                        {!termoContrato}
                    </aura:iteration> -->
                </lightning:card>
                
                <div class="contrato-aceitar" >
                    <lightning:input type="checkbox" onclick="{!c.aceitarContrato}"/> <div style="margin:10px">  Declaro que li as cláusulas acima do termo de aceite para o Loxam Club, estando ciente de sua existência e desejo fazer parte do Loxam Club, bem como aceito a política de privacidade e tratamento de dados da Loxam Degraus.</div> 
                </div>                
            </lightning:card>
          </aura:if>

          <aura:if isTrue="{!v.contratoAceito}">
            <lightning:card class="contrato-finish">
              <div class="contrato-finish-texto">
                <p>Parabéns! Agora você está participando do Programa Loxam Club!</p>
                <p>Atenção, verifique no seu e-mail cadastrado um presente de boas vindas que você recebeu. <lightning:icon iconName="action:share_thanks" size="small" alternativeText="Indicates approval" /></p>
                <center>
                  <div class="icon-email">
                      <lightning:icon style="width: 100px;" iconName="utility:email" size="large" alternativeText="Indicates approval" />
                  </div>
                </center>

                <p>Atenciosamente,</p>
                <p>Loxam Degraus</p>
              </div>
                <div class="contrato-finish-logo">
                    <img style="max-width: 100%;
                                width: 200px;
                                margin: 0 auto;
                                display: block;"
                    src="data:image/png;base64," />
                </div>
            </lightning:card >
          </aura:if>

        </div>
    </aura:if>  
    
</aura:component>